{:datomic {:uri #or [#env DATOMIC_URI "datomic:mem:clojurians_log"]}
 :http {:port #long #or [#env CLOJURIANS_LOG_PORT 4983]
        :origin #or [#env CLOJURIANS_LOG_HOSTNAME "http://localhost:4983"]}
